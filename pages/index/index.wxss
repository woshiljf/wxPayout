
.home-container{
  background-color: rgb(236, 62, 52);
  margin-top: 80px;
}
/* 头部定位start */
.locationHeader{
  height: 100rpx;
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  padding-left: 25rpx;
  padding-right: 25rpx;
  box-sizing: border-box;
}
.location{
  display: flex;
  justify-content:flex-start;
}
/* 头部定位end */
/* 汇率start */
.conponHeader{
  height: 100px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  margin-left: 25rpx;
  margin-right: 25rpx;
  border-radius: 25rpx 25rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.button{
  width: 150rpx;
  height: 70rpx;
  background-color: red;
  border-radius: 70rpx 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: white;
}
.conponMore{
  display: flex;
  justify-content: space-between;
  border-left: 1px solid #ccc;
  align-content: center;
}

/* 汇率end */

/* 内容start */
.content-container{
  width: 100%;
  height: 500px;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
  border-radius: 15rpx 15rpx 0 0 ;
  background-color: #fff;
}
.service-container{
  padding-top: 20rpx;
}

.contry-content{
  margin-top: 25rpx;
}
.tab-class{
  border: 1px solid #000;
  border-radius: 10rpx 10rpx;
}





/* 内容end */