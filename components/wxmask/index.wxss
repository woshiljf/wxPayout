.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.6;
 }
  
 /* 第一张弹出优惠券start */
 .modalDlg{
  z-index: 9999;
  color: white;
  text-align: center;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
 }

.topText1Box {
  display: flex;
  width: 471rpx;
  height: 560rpx;
  position: relative;
}

.left1:after{
  content: '';
  position: absolute;
  bottom: 0px;
  right: 15rpx;
  width: 80%;
  height: 5rpx;
  background: radial-gradient(circle, #fff, #fff 4px, transparent 5px);
  background-size: 10px 10px;
}

.right1:after{
  content: '';
  position: absolute;
  bottom: 0px;
  right: 15rpx;
  width: 50%;
  height: 5rpx;
  background: radial-gradient(circle, #fff, #fff 4px, transparent 5px);
  background-size: 10px 10px;
}


.left1{
  width: 50%;
  border-radius: 10rpx 0rpx 0 0;
  background-image: radial-gradient(circle at left bottom,transparent,transparent 10px,orange 11px);
  position: relative;
  
}
.right1{
  width: 50%;
  border-radius: 0rpx 10rpx 0 0;
  background-image: radial-gradient(circle at right bottom,transparent,transparent 10px,orange 11px);
}

.textContent1{
  width: 471rpx;
  height: 450rpx;
  position: absolute;
  top: 50rpx;
}
.textContent1::before{
  content: '恭喜你获得:';
  display: block;
  font-family: PingFangSC-Semibold;
  font-size: 42rpx;
  color: #fff;
  letter-spacing: 0;
  height:  100rpx;
  width: 100%;
  left: -120rpx;
  position: absolute;
  top: -150rpx
}


.text1{
  font-family: PingFangSC-Medium;
font-size: 32rpx;
color: #FFFFFF;
letter-spacing: 0;
text-align: center;

}
.text2{
  margin-top: 10rpx;
  font-family: PingFangSC-Semibold;
  font-size: 42rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
}
.text3{
  width: 410rpx;
  height: 198rpx;
  background-color: #fff;
  text-align: center;
  padding-top: 50rpx;
  box-sizing: border-box;
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translateX(-50%);

}
