.contentTitle{
  display: flex;
  justify-content: space-between;
  padding-right: 50rpx;
}
.contentTitle-one{
  display: flex;
  justify-content: space-between;
}

.youhuiBox1{
  box-sizing: border-box;
}
.you11{
  width: 673rpx;
  height: 230rpx;
  background-color: #FF8F00;
  border-radius: 2% 2% 0 0;
  border-bottom: 1px dashed #fff;
  position: relative;
  margin-top: 50rpx;
  width: 300rpx;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.you22{
  width: 673rpx;
  height: 104rpx;
  background-color: #FFFFFF;
  border: 1px solid #fff;
  border-radius: 0% 0% 2% 2%;
  box-shadow: 0 15rpx 10rpx -15rpx #999;
  border-top: 1px dashed #fff;
  font-size: small;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 300rpx;
}
.you11::before{
  content: '';
  display: block;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  border-radius: 50%;
  background-color: #FFFFFF;
  left: -9rpx;
  bottom: -15rpx;
  z-index: 1;
}
.you11::after{
  content: '';
  display: block;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  border-radius: 50%;
  background-color: #fff;
  right: -8rpx;
  bottom: -15rpx;
  z-index: 1;
}

